<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="E2E" />

		</run>
	</groups>
	<listeners>
		<listener
			class-name="SeleniumPractice.SeleniumHybridFrameworkNew.testcomponents.Listeners" />

	</listeners>
	<test thread-count="5" name="Product Catalogue Test">
		<classes>
			<class
				name="SeleniumPractice.SeleniumHybridFrameworkNew.TC_ProductCatalogueTest" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Submit Order Test">
		<classes>
			<class
				name="SeleniumPractice.SeleniumHybridFrameworkNew.TC_SubmitOrderTest" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Cart Page Test">
		<classes>
			<class
				name="SeleniumPractice.SeleniumHybridFrameworkNew.TC_CartPageTest" />
		</classes>
	</test> <!-- Test -->


</suite> <!-- Suite -->
